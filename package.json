{
  "name": "skill-evals",
  "version": "1.0.0",
  "type": "module",
  "description": "Testing framework for evaluating agent skills with and without skill augmentation",
  "main": "dist/index.js",
  "scripts": {
    "compare": "tsx scripts/run-comparison.ts",
    "compare:debug": "tsx scripts/run-comparison.ts --debug",
    "compare:dry": "tsx scripts/run-comparison.ts --dry",
    "eval:baseline": "tsx scripts/run-comparison.ts --experiment baseline",
    "eval:with-skill": "tsx scripts/run-comparison.ts --experiment with-skill",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
